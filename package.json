{
  "name": "jumpselect",
  "displayName": "jumpselect",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jumpselect.jumpRight",
        "title": "Jump Right",
        "category": "JumpSelect"
      },
      {
        "command": "jumpselect.jumpLeft",
        "title": "Jump Left",
        "category": "JumpSelect"
      },
      {
        "command": "jumpselect.jumpSelectLeft",
        "title": "Jump and Select Left",
        "category": "JumpSelect"
      },
      {
        "command": "jumpselect.jumpSelectRight",
        "title": "Jump and Select Right",
        "category": "JumpSelect"
      }
    ],
    "keybindings": [
      {
        "command": "jumpselect.jumpRight",
        "key": "cmd+.",
        "when": "editorTextFocus"
      },
      {
        "command": "jumpselect.jumpSelectRight",
        "key": "shift+cmd+.",
        "when": "editorTextFocus"
      },
      {
        "command": "jumpselect.jumpLeft",
        "key": "cmd+,",
        "when": "editorTextFocus"
      },
      {
        "command": "jumpselect.jumpSelectLeft",
        "key": "shift+cmd+,",
        "when": "editorTextFocus"
      }
    ],
    "configuration": [
      {
        "title": "jumpselect",
        "type": "string",
        "properties": {
          "jumpselect.showInputBox": {
            "type": "boolean",
            "scope": "resource",
            "default": false,
            "description": "Whether to take input from the input box. Requires an extra key press, but won't conflict with other extensions that use the 'type' command."
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2",
    "@vscode/test-electron": "^2.3.6"
  }
}